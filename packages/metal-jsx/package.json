{
  "name": "metal-jsx",
  "version": "2.14.1",
  "description": "A JSX templates renderer to be used with Metal.js's Component class",
  "license": "BSD",
  "repository": "https://github.com/metal/metal.js/tree/master/packages/metal-jsx",
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "jsnext:main": "src/all/jsx.js",
  "main": "lib/all/jsx.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "compile": "babel --no-babelrc --presets env -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "keywords": [
    "metal"
  ],
  "dependencies": {
    "metal": "^2.14.0",
    "metal-component": "^2.14.1",
    "metal-dom": "^2.14.0",
    "metal-incremental-dom": "^2.14.1",
    "metal-state": "^2.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-env": "^1.6.0",
    "babel-preset-metal-jsx": "^0.0.2"
  }
}
